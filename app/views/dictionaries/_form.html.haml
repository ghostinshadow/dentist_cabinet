= form_for @dictionary do |f|
  - if @dictionary.errors.any?
    #error_explanation
      %h2= "#{pluralize(@dictionary.errors.count, "error")} prohibited this dictionary from being saved:"
      %ul
        - @dictionary.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :resource_type
    = f.text_field :resource_type
  .actions
    = f.submit 'Save'
